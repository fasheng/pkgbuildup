pkgname=hello
pkgver=2.8
pkgrel=4
pkgdesc='Hello package from Debian'
arch=('i686' 'x86_64')
depends=()
license=('GPL3')
provides=("${pkgname}")
url="http://www.debian.com/"
_parent_url="http://ftp.debian.org/debian/pool/main/h/hello"
_filename_x86="hello_2.8-4_i386.deb"
_filename_x64="hello_2.8-4_amd64.deb"
_md5_x86="df64b44fa73e6c7f69c1df79cbc72a14"
_md5_x64="394412641fe84292991b28744a90234f"
if test "$CARCH" == i686; then
    _filename=${_filename_x86}
    _md5=${_md5_x86}
else    
    _filename=${_filename_x64}
    _md5=${_md5_x64}
fi
source=("${_parent_url}/${_filename}")
md5sums=("${_md5}")

package() {
    tar xvf ${srcdir}/data.tar.xz -C ${pkgdir}/
}
